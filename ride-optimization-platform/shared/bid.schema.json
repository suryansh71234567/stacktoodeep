{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Bid",
    "type": "object",
    "required": [
        "bid_id",
        "bundle_id",
        "bidder_id",
        "proposed_profit_share",
        "valid_until"
    ],
    "properties": {
        "bid_id": {
            "type": "string"
        },
        "bundle_id": {
            "type": "string"
        },
        "bidder_id": {
            "type": "string"
        },
        "proposed_profit_share": {
            "type": "number",
            "description": "Percentage offered to broker"
        },
        "valid_until": {
            "type": "string",
            "description": "ISO timestamp"
        }
    }
}